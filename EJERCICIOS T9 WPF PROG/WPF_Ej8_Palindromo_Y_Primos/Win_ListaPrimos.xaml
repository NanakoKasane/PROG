<Window x:Class="WPF_Ej8_Palindromo_Y_Primos.Win_ListaPrimos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Lista de números primos" Height="300" Width="385.185">
    <Grid Name="grd_Primos" Loaded="Grid_Loaded_1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="175*"/>
            <ColumnDefinition Width="117*"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF00F3FF" Offset="1"/>
                <GradientStop Color="#FF22D484"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label Name="lblTitulo" FontSize="15" FontWeight="Bold" Margin="10 10,10,0" Content="LISTA DE NÚMEROS PRIMOS MENORES QUE " Grid.ColumnSpan="2"/>

    </Grid>
</Window>
