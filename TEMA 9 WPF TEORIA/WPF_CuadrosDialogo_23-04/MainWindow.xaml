<Window x:Class="WPF_CuadrosDialogo_23_04.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Cuadros de diálogo" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Ir al cuadro de diálogo" HorizontalAlignment="Left" Margin="208,30,10,10" VerticalAlignment="Top" Width="190" Height="51" Click="Button_Click"/>
            <Button Content="Abrir Archivo" x:Name="btnAbrirArchivo" Height="40" Margin="208,30,192,30" Click="btnAbrirArchivo_Click"/>
            <Button Content="Guardar Archivo" x:Name="btnGuardarArchivo" Height="40" Margin="208,10,192,30" Click="btnGuardarArchivo_Click"/>
            <Button Content="Imprimir" x:Name="btnImprimir" Height="40" Margin="208,0,192,30" Click="btnImprimir_Click"/>

        </StackPanel>
    </Grid>
</Window>
